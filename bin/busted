#!/bin/sh

exec '/opt/neovim-deps/bin/luajit' -e 'package.path="/opt/neovim-deps/share/lua/5.1/?.lua;/opt/neovim-deps/share/lua/5.1/?/init.lua;"..package.path; package.cpath="/opt/neovim-deps/lib/lua/5.1/?.so;"..package.cpath' -e 'local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("busted","2.0.rc3-0")' '/opt/neovim-deps/lib/luarocks/rocks/busted/2.0.rc3-0/bin/busted' "$@"
